# Environment Configuration

# API Settings
APP_NAME=Holly and Morty API
APP_VERSION=1.0.0
DEBUG=false

# ElevenLabs Settings
# Get your webhook secret from ElevenLabs dashboard
ELEVENLABS_WEBHOOK_SECRET=your_elevenlabs_webhook_secret_here
# Get your API key from ElevenLabs dashboard
XI_API_KEY=your_elevenlabs_api_key_here

# Azure Cosmos DB Settings
# Connection string format: AccountEndpoint=https://YOUR_ACCOUNT.documents.azure.com:443/;AccountKey=YOUR_KEY;
COSMOS_CONNECTION_STRING=your_cosmos_connection_string_here
COSMOS_DATABASE_NAME=holly-morty-db

# Anthropic Settings
# Get your API key from https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-sonnet-4-5-20250929
